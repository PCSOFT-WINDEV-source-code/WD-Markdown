  procedures :
   -
     name : OnNewTab
     procedure_id : 1434955638838740401
     type_code : 14
     code : |1-
      // Summary: Executed when a tab is opened
      // Syntax:
      // OnNewTab ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      procedure private OnNewTab( TabCtrl is Control )
      
      if TAB_MD.Count > 0 then
      	TAB_MD.Visible = True
      end
      
      // Stores
      Editors[ TabCtrl..Alias ] <- TabCtrl
      
      TabCtrl.IW_EDITOR.AssociatedTab = TabCtrl
     type : 458752
   -
     name : OnTabClose
     procedure_id : 1434955638838805937
     type_code : 14
     code : |1-
      // Summary: Triggered when a tab is closed
      // Syntax:
      //OnTabClose (<Tab> is Control)
      //
      // Parameters:
      //	Tab (Control): Tab
      // Return value:
      // 	None
      //
      procedure private OnTabClose( TabCtrl is Control )
      
      // Notifies the editor that the tab has been closed
      if TabCtrl <> Null then
      	if TabCtrl.IW_EDITOR.OnClose()	= False then
      		SetFocusAndReturnToUserInput(WIN_MARKDOWN)
      	end
      end
      
      // Stores
      Delete(Editors, TabCtrl..Alias)
      
      if TAB_MD.Count <= 1 then
      	TAB_MD.Visible = False
      end
     type : 458752
