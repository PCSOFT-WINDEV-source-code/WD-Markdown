      procedure MyWindow()
      
      Editors is ASSOCIATIVE ARRAY of champ
      
      ExplorerAccept( True, MyWindow, procedure( nNbFiles is int, sListOfFiles is string ) { 
      
      // Only the first file is taken into account
      for each string sFile of sListOfFiles separee by CR
      	
      	if fFileExist(sFile) then
      		
      		// Loads the file
      		OnNewTab( TAB_MD.Open(fExtractPath(sFile, fFile + fExtension), IW_EDITOR, sFile) )			
      	end	
      end
      } )
